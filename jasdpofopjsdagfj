local library = loadstring(game:HttpGet("https://raw.githubusercontent.com/skid7543w7hdghedty/zsfdgpijtrhyijklsdfg/refs/heads/main/vcxhukhusdfhljk"))()

library:init() -- Initalizes Library Do Not Delete This

local Window = library.NewWindow({
	title = "Gamesense.pub | Skeet.cc",
	size = UDim2.new(0, 525, 0, 650)
})

local tabs = {
    Main = Window:AddTab("Main"),
	Settings = library:CreateSettingsTab(Window),
}

-- 1 = Set Section Box To The Left
-- 2 = Set Section Box To The Right

local sections = {
	LocalPlayer = tabs.Main:AddSection("LocalPlayer", 1),
	Scripts = tabs.Main:AddSection("Scripts", 2),
}

sections.LocalPlayer:AddSlider({
	text = "WalkSpeed", 
	flag = 'Slider_1', 
	suffix = "", 
	value = 16,
	min = 1, 
	max = 1000,
	increment = 0.1,
	tooltip = "Anti-Cheat Detected",
	risky = true,
	callback = function(Value) 
		game.Players.LocalPlayer.Character.Humanoid.WalkSpeed = Value
	end
})

sections.LocalPlayer:AddSlider({
	text = "JumpPower", 
	flag = 'Slider_1', 
	suffix = "", 
	value = 16,
